{
  "name": "wasm-benchmark",
  "version": "1.0.0",
  "type": "module",
  "description": "WebAssembly Data Transfer Benchmark - Comparing embind, JSON (yyjson), MessagePack, and typed-cstruct",
  "main": "benchmark.mjs",
  "scripts": {
    "build": "mkdir -p build && cd build && emcmake cmake .. && emmake make",
    "build:clean": "rm -rf build && npm run build",
    "benchmark": "node benchmark.mjs",
    "benchmark:flat": "node benchmarks/flat.mjs",
    "benchmark:nested": "node benchmarks/nested.mjs",
    "benchmark:numbers": "node benchmarks/numbers.mjs",
    "benchmark:objects": "node benchmarks/objects.mjs",
    "benchmark:tree": "node benchmarks/tree.mjs",
    "test": "node benchmark.mjs"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "benchmark",
    "embind",
    "json",
    "messagepack",
    "typed-cstruct"
  ],
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "typed-cstruct": "^0.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
